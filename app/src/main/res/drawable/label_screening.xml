<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 标签选中时的背景 -->
    <item android:state_selected="true">
        <shape>
            <stroke android:width="2dp" android:color="#E9F5FE" />
            <corners android:radius="@dimen/sp_14" />
            <solid android:color="#E9F5FE" />
        </shape>
    </item>
    <!-- 标签的正常背景 -->
    <item>
        <shape>
            <stroke android:width="2dp" android:color="#F4F4F9" />
            <corners android:radius="@dimen/sp_14" />
            <solid android:color="#F4F4F9" />
        </shape>
    </item>
</selector>
