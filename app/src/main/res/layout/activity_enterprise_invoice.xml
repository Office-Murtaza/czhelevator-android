<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    >
    <include
        android:id="@+id/ll_top1"
        layout="@layout/layout2_top"/>
    <LinearLayout
        android:layout_below="@id/ll_top1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/dp10"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="发票抬头："
                android:textColor="#ff333333"
                android:textSize="14sp"
                />

            <EditText
                android:id="@+id/et_top"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="请输入发票抬头"
                android:textColor="#333333"
                android:textSize="14sp"
                android:padding="@dimen/dp_15"
                android:background="@color/color_00000000"
                />

        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp1"
            android:background="#e1e1e1"
            />


        <LinearLayout
            android:id="@+id/ll_qy_number"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"

            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="企业税号："
                android:textColor="#ff333333"
                android:textSize="14sp"
                />

            <EditText
                android:id="@+id/et_qy_number"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="请输入企业税号"
                android:textColor="#333333"
                android:textSize="14sp"
                android:padding="@dimen/dp_15"
                android:inputType="number"
                android:background="@color/color_00000000"
                />

        </LinearLayout>
        <TextView
            android:id="@+id/tv_qy_number"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp1"
            android:background="#e1e1e1"
            />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="开票内容："
                android:textColor="#ff333333"
                android:textSize="14sp"
                />

            <TextView
                android:id="@+id/et_conent"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="广告服务费"
                android:textColor="#333333"
                android:textSize="14sp"
                android:padding="@dimen/dp_15"
                android:background="@color/color_00000000"
                />

        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp1"
            android:background="#e1e1e1"
            />



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="开票金额："
                android:textColor="#ff333333"
                android:textSize="14sp"
                />

            <EditText
                android:id="@+id/et_money"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="当前最多可开2300.00"
                android:textColor="#333333"
                android:textSize="14sp"
                android:padding="@dimen/dp_15"
                android:inputType="number"
                android:background="@color/color_00000000"
                />

            <TextView
                android:id="@+id/tv_all"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="全部"
                android:textColor="#ffff3049"
                android:textSize="14sp"
                android:background="@drawable/bg_pay_code"
                android:paddingBottom="@dimen/dp5"
                android:paddingRight="@dimen/dp10"
                android:paddingLeft="@dimen/dp10"
                android:paddingTop="@dimen/dp5"
                />

        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp1"
            android:background="#e1e1e1"
            />



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="电子邮箱："
                android:textColor="#ff333333"
                android:textSize="14sp"
                />

            <EditText
                android:id="@+id/et_email"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="请输入电子邮箱"
                android:textColor="#333333"
                android:textSize="14sp"
                android:padding="@dimen/dp_15"
                android:inputType="textEmailAddress"
                android:background="@color/color_00000000"
                />

        </LinearLayout>

    </LinearLayout>

    <TextView
        android:id="@+id/tv_login_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@mipmap/btn_common_normal"
        android:text="提交"
        android:textColor="#ffffffff"
        android:textSize="14sp"
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/dp_40"
        />

</RelativeLayout>